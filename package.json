{
  "name": "storeapp",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "build": "tsc -b --verbose && shx --verbose cp -ru public dist/",
    "bsw": "npm run build  && npm run start:watch",
    "start:watch": "nodemon",
    "watch": "jest --watch --coverage",
    "test": "jest --colors --coverage",
    "migrate:pistes": "ts-node scripts/migratePisteData.ts",
    "generate:popularity": "ts-node scripts/generatePopularityData.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src",
      "public"
    ],
    "exec": "npm run build && npm start",
    "ext": "ts, js"
  },
  "dependencies": {
    "csv-parse": "^5.6.0",
    "datatables.net": "^1.13.6",
    "datatables.net-dt": "^1.13.6",
    "debug": "^2.6.9",
    "express": "^4.19.2",
    "express-flash-plus": "^0.0.7",
    "express-session": "^1.17.2",
    "http-errors": "^1.6.3",
    "jest-runner": "^29.7.0",
    "jquery": "^3.7.1",
    "leaflet": "^1.7.1",
    "mongoose": "^8.12.1",
    "morgan": "^1.9.1",
    "node-fetch": "^3.3.2",
    "papaparse": "^5.5.1",
    "pug": "^3.0.3"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.17.14",
    "@types/papaparse": "^5.3.15",
    "@types/supertest": "^6.0.2",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "nodemon": "^3.1.4",
    "shx": "^0.3.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
